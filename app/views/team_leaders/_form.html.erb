<%= simple_form_for(@team_leader, :remote => true) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :number_of_workers %>
  </div>
	<%= f.simple_fields_for :user do |builder|%>
	<%= render 'devise/registrations/details', u: builder%>
	<% end %>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
